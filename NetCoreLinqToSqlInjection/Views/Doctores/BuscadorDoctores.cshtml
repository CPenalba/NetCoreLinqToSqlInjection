@model List<Doctor><h1>Lambda expression Linq</h1><form method="post">    <label>Especialidad: </label>    <input type="text" name="Especialidad"/>    <button class="btn btn-info">        Buscar doctores    </button></form>@if (Model != null)
{    <table class="table table-bordered">        <thead>            <tr>                <th>Hospital cod</th>                <th>Doctor no</th>                <th>Apellido</th>                <th>Especialidad</th>                <th>Salario</th>            </tr>        </thead>        <tbody>            @foreach (Doctor d in Model)
            {                <tr>                    <td>@d.IdHospital</td>                    <td>@d.IdDoctor</td>                    <td>@d.Apellido</td>                    <td>@d.Especialidad</td>                    <td>@d.Salario</td>                </tr>            }        </tbody>    </table>}