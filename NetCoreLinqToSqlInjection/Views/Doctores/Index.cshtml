@model List<Doctor> <h1>Doctores SQL Server/Oracle</h1><p>    <a asp-controller="Doctores"    asp-action="Create">        Create New Doctor    </a></p><table class="table table-secondary">    <thead>        <tr>            <th>Id doctor</th>            <th>Apellido</th>            <th>Especialidad</th>            <th>Salario</th>            <th>Hospital</th>            <th>Delete</th>            <th>Edit</th>        </tr>    </thead>    <tbody>        @foreach (Doctor doctor in Model){            <tr>                <td>@doctor.IdDoctor</td>                <td>@doctor.Apellido</td>                <td>@doctor.Especialidad</td>                <td>@doctor.Salario</td>                <td>@doctor.IdHospital</td>                <td> <a class="btn btn-danger" asp-controller="Doctores" asp-action="Delete" asp-route-iddoctor="@doctor.IdDoctor">Delete</a></td>                 <td> <a class="btn btn-info" asp-controller="Doctores" asp-action="Edit" asp-route-iddoctor="@doctor.IdDoctor">Edit</a></td>            </tr>        }    </tbody></table>
